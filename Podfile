# platform :ios, '9.0'

target 'DigBig' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DigBig
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database' 
  pod 'Firebase/Storage'

  # Pods for Facebook
  pod 'FBSDKLoginKit'

  #Pods for Google
  pod 'GoogleSignIn'

end
